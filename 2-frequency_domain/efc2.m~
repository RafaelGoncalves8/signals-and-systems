x = @(n,w) cos(n*w);
N = 1000;
vec = [];

% a)
w = pi/2;
for i = 0:N,
    X = [vec; x(i,w)];
end;
Y = sistema_desconhecido(X)
plot(X)
hold on
plot(Y, 'm')


% b)
w = pi/200;
for i = 0:N,
    X = [vec; x(i,w)];
end;
Y = sistema_desconhecido(X)
stem(Y)
hold on

w = pi/20;
for i = 0:N,
    X = [vec; x(i,w)];
end;
Y = sistema_desconhecido(X)
stem(Y, 'm')
legend(["w = pi/200", "w = pi/20"

% c)

